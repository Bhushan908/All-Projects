@model IEnumerable<tradirouting.Models.student>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>getallstudents</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
